    },
    "autoload-dev": {
        "psr-4": {
            "ZasDev\\RSSTest\\": "module/RSS/tests",
            "ZasDev\\Mock\\": "mocks",
            "ZasDev\\Common\\": "module/ZasDev/Common/src",
            "ZasDev\\Ajax\\": "module/ZasDev/Ajax/src"
