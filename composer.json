        "ircmaxell/password-compat": "1.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "squizlabs/php_codesniffer": "1.*",
        "ccampbell/chromephp": "4.1.*",
        "zendframework/zend-developer-tools": "dev-master",
        "zasdev/ajax": "dev-develop",
        "zasdev/common": "dev-develop"
